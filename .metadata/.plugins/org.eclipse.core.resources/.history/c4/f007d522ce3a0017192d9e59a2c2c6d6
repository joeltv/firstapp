package Domain;

public class Nummer {
	private int ID;
	private String Titel;
	private int Album_ID;
	
	public Nummer(int ID, String Titel,  int Album_ID){
		this.ID = ID;
		this.Titel = Titel;
		this.Album_ID = Album_ID;
	}
	
	public int getID() {
		return ID;
	}
	
	public void setID(int ID) {
		this.ID = ID;		
	}
	
	public int getTitel() {
		return ID;
	}
	
	public void setTitel(String Titel) {
		this.Titel = Titel;		
	}
	
	public int getAlbum_ID() {
		return Album_ID;
	}
	
	public void setAlbum_ID(int Album_ID) {
		this.Album_ID = Album_ID;		
	}
}
